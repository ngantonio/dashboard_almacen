        "laravel-mix": "^1.0",
        "lodash": "^4.17.4",
        "vue": "^2.5.7",
        "vue-resource": "^1.5.1"
    },
    "dependencies": {
        "chart.js": "^2.8.0",
        "laravel-echo": "^1.5.3",
